{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "playground": {
            "projectType": "application",
            "schematics": {},
            "root": "",
            "sourceRoot": "src/webapp",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "lib/webapp",
                        "index": "src/webapp/index.html",
                        "main": "src/webapp/main.ts",
                        "polyfills": "src/webapp/polyfills.ts",
                        "tsConfig": "tsconfig.webapp.build.json",
                        "aot": true,
                        "assets": [
                            "src/webapp/favicon.ico",
                            "src/webapp/assets"
                        ],
                        "styles": [
                            "src/webapp/styles.css"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/webapp/environments/environment.ts",
                                    "with": "src/webapp/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "playground:build",
                        "proxyConfig": "proxy.conf.json"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "playground:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "playground:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/webapp/test.ts",
                        "polyfills": "src/webapp/polyfills.ts",
                        "tsConfig": "tsconfig.webapp.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": [
                            "src/webapp/favicon.ico",
                            "src/webapp/assets"
                        ],
                        "styles": [
                            "src/webapp/styles.css"
                        ],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "tsconfig.webapp.build.json",
                            "tsconfig.webapp.spec.json",
                            "e2e/tsconfig.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "playground:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "playground:serve:production"
                        }
                    }
                }
            }
        }
    },
    "defaultProject": "playground"
}
